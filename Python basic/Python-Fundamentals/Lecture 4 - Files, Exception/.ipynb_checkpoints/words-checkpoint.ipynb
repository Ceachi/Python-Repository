    "def fetch_words():\n",
    "\n",
    "   with open('t.txt') as file:\n",
    "\n",
    "       file_words = []\n",
    "\n",
    "       for line in file:\n",
    "\n",
    "           line_words = line.split()\n",
    "\n",
    "           for word in line_words:\n",
    "\n",
    "               file_words.append(word)\n",
    "\n",
    "   for word in file_words:\n",
    "\n",
    "       print(word)"
